<head>
  <meta name="viewport" content="width=device-width,initial-scale=1">
  <%= csrf_meta_tags %>
  <%= csp_meta_tag %>
  <%= stylesheet_link_tag "application", "data-turbo-track": "reload" %>
  <%= javascript_importmap_tags %>
  <script src="https://kit.fontawesome.com/4177f37136.js" crossorigin="anonymous"></script>
</head>
<div>
  <%= render "partials/header" %>
  <div class="container">
    <div class="row">
      <div class="col-md-2">
        <%= render "partials/sidebar" %>
      </div>
      <div class="col-md-10">
        <%= yield %>
      </div>
    </div>
  </div>
</div>