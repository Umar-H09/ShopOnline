<div>
  <div class="container pt-5">
    <div class="row">
      <div class="col-md-4">
      </div>
      <div class="col-md-4">
        <%= form_with model: @user, url: vendors_path do |f| %>
          <div class="form-group">
            <%= f.label :username %>
            <%= f.text_field :username, class: "form-control", placeholder:"Enter name ..." %>
          </div>
          <div class="form-group">
            <%= f.label :email %>
            <%= f.text_field :email, class: "form-control",placeholder:"Enter email ..." %>
          </div>
          <div class="form-group">
            <%= f.label :password %>
            <%= f.text_field :password,type:'password', class: "form-control",placeholder:"Enter Password..." %>
          </div>
          <%= f.submit class:'btn btn-danger d-block mx-auto'%>
        <%end%>
      </div>
    </div>
  </div>
</div>